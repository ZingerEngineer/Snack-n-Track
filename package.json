{
  "version": "1.0.0",
  "name": "snack_n_track",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:web\"",
    "server": "npm run -w apps/server dev",
    "web": "npm run -w apps/web dev",
    "server_mobile": "concurrently \"npm run -w apps/server dev\" \"npm run -w apps/mobile dev\""
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "eslint": "^9.15.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "zod": "3.23.8"
  },
  "dependencies": {}
}
